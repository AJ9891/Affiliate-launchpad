# Vite environment variables for Affiliate Launchpad
# Copy this file to `.env` and fill in real values. Do NOT commit `.env`.

# Base SendShark API URL (include trailing slash or not, both work)
VITE_SENDSHARK_API_URL="https://api.sendshark.com/v1"
# Your SendShark API key
VITE_SENDSHARK_API_KEY="REPLACE_ME"
# List ID for the Launchpad list
VITE_SENDSHARK_LIST_ID="REPLACE_ME"

# From / reply-to details used in messaging (optional in this demo)
VITE_FROM_NAME="Abby"
VITE_FROM_EMAIL="noreply@example.com"

# Lead magnet direct download link (e.g. https://yourcdn.com/quickstart.pdf)
VITE_LEAD_MAGNET_DOWNLOAD="https://example.com/lead-magnet.pdf"
